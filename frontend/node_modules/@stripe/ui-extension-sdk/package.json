{
  "name": "@stripe/ui-extension-sdk",
  "version": "8.0.0",
  "description": "The suite of functionality available to UI extensions in Stripe apps",
  "author": "Stripe, Inc.",
  "license": "MIT",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx"
    }
  },
  "scripts": {
    "bind": "$npm_execpath run tsm bind",
    "bind-all": "$npm_execpath run tsm bind-all",
    "build": "$npm_execpath run tsm build && type yalc &>/dev/null || exit 0 && cd ./dist && yalc push --changed",
    "clean": "rm -r ./dist",
    "integrity": "yarn-integrity",
    "lint": "eslint --ext ts,tsx src",
    "prepare": "$npm_execpath run integrity",
    "release": "$npm_execpath run tsm release",
    "start": "npm-watch build",
    "test": "jest --watchAll",
    "tsm": "tsm",
    "version-update": "$npm_execpath run tsm version-update"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@remote-ui/react": "^4.4.5",
    "@remote-ui/testing": "^1.3.6",
    "@types/stylis": "^4.0.2",
    "invariant": "^2.2.4",
    "react": "^17.0.2",
    "stripe": "^8.174.0"
  },
  "peerDependencies": {
    "@remote-ui/rpc": "^1.2.5"
  },
  "devDependencies": {
    "@sail/icons": "^0.0.12",
    "@sail/ui": "31.4.1",
    "@stripe-internal/eslint-config-stripe": "^2.0.1",
    "@stripe-internal/npm-publish": "^4.1.0",
    "@stripe-internal/tailor-dashboard-components": "2.1.1",
    "@stripe-internal/tailor-tsm": "1.1.0",
    "@stripe-internal/yarn-integrity": "^0.0.96",
    "@swc/core": "1.2.121",
    "@swc/jest": "^0.2.20",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^27.0.3",
    "@types/react-reconciler": "^0.26.1",
    "@typescript-eslint/eslint-plugin": "^5.14.1-alpha.6",
    "@typescript-eslint/parser": "^5.14.1-alpha.6",
    "chokidar": "^3.5.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "flowgen": "^1.14.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.4",
    "jest-junit": "^13.0.0",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "ts-expect": "^1.3.0",
    "tsc-alias": "^1.3.7",
    "typescript": "^4.5.0-dev.20211206"
  },
  "keywords": [
    "stripe",
    "dashboard",
    "extensions",
    "sdk"
  ]
}